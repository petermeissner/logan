<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.petermeissner.logan.App">

  <MenuBar VBox.vgrow="NEVER">

    <Menu mnemonicParsing="false" text="File">
      <MenuItem mnemonicParsing="false" text="Dummy" />
    </Menu>

    <Menu mnemonicParsing="false" text="Help">
      <MenuItem mnemonicParsing="false" text="About MyHelloApp"/>
    </Menu>

  </MenuBar>




  <SplitPane dividerPositions="0.1731171548117155" prefHeight="202.0" prefWidth="640.0" VBox.vgrow="ALWAYS">

    <VBox fillWidth="false" prefHeight="382.0" prefWidth="315.0" maxWidth="300.0">

      <opaqueInsets>
        <Insets />
      </opaqueInsets>

      <Button mnemonicParsing="false" onAction="#loadMyShapes" text="MyShapes">
        <VBox.margin>
          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </VBox.margin>
      </Button>

      <Button mnemonicParsing="false" onAction="#LoadMasterDetail" text="MasterDetail">
        <VBox.margin>
          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </VBox.margin>
      </Button>

    </VBox>

    <ScrollPane fx:id="mainSceneContent" prefHeight="643.0" prefWidth="316.0">
      <Label>Placeholder</Label>
    </ScrollPane>

  </SplitPane>


</VBox>
