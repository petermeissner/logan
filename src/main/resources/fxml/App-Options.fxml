<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>

<SplitPane
        dividerPositions="0.08640939597315436"
        maxHeight="-Infinity"
        maxWidth="-Infinity"
        minHeight="-Infinity"
        minWidth="-Infinity"
        orientation="VERTICAL"
        prefHeight="400.0"
        prefWidth="600.0"
        xmlns="http://javafx.com/javafx/17.0.2-ea"
        xmlns:fx="http://javafx.com/fxml/1"
>

    <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button mnemonicParsing="false" text="Load" onAction="#loadOptions"/>
            <Button mnemonicParsing="false" text="Save" onAction="#storeOptions"/>
        </buttons>
    </ButtonBar>

    <ScrollPane prefHeight="200.0" prefWidth="200.0">
        <TextArea fx:id="optionsText" editable="true" prefHeight="346.0" prefWidth="598.0" promptText="logs"/>
    </ScrollPane>

</SplitPane>
